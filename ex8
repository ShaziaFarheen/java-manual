class square 
{
    void product(int x, int y)
    {
        System.out.println("The product of " + x + " and " + y + " is " + (x*y));     
    }
    void product(float x, float y)
    {
        System.out.println("The product of " + x + " and " + y + " is " + (x*y));
    }
}
public class Main
{
    public static void main(String args[])
    {
        square a = new square();
        a.product(5,29);
        a.product(4.2f,13.5f);
    }
}
