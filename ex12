class sample
{
    int reg;
    String name;
    void display()
    {
        System.out.println("Reg No : " + reg);
        System.out.println("Name : " + name);
    }
}
public class Main
{
    public static void main(String args[])
    {
        sample s1 = new sample();
        sample s2 = new sample();
        s1.display();
        s2.display();
    }
}
